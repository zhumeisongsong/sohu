---
title: "登录"
---

<div class="mui-content">

  <section class="photo-con">
    <img src="../image/user@3x.png">
  </section>

  <form id='login_form' class="mui-input-group form-input">
    <div class="mui-input-row ui-border-b">
      <input id='account' type="text" class="mui-input-clear mui-input " placeholder="输入手机号" maxlength="20">

    </div>
    <div class="mui-input-row">
      <input id='password' type="password" class=" mui-input-clear mui-input input-mode" placeholder="输入密码" maxlength="20">
    </div>
  </form>
  <div class="mui-content-padded">
    <button id='login' type="button" class="mui-btn mui-btn-block mui-red">登录</button>

    <div class="link-area">
      <span id='reg' class="link-cell">注册账号</span>
      <span class="spliter">|</span>
      <span id='forgetPassword' class="link-cell">忘记密码</span>
    </div>
  </div>

</div>